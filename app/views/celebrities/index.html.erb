<p id="notice"><%= notice %></p>

<h1><%= t ".title" %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @celebrities.each do |celebrity| %>
      <tr>
        <td><%= celebrity.name %></td>
        <td><%= image_tag celebrity.portrait.url if celebrity.portrait? %> </td>
        <td><%= link_to t(".show"), celebrity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
